{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "next lint",
    "test": "npm run test:node && npm run test:jsdom",
    "test:jsdom": "jest --testEnvironment=jsdom --testMatch '**/*.test.tsx'",
    "test:node": "jest --testEnvironment=node --testMatch '**/*.test.ts'",
    "build:db": "rm -rf dist && tsc --project tsconfig.sequelize.json",
    "migrate": "NODE_ENV=development sequelize-cli db:migrate --migrations-path dist/db/migrations --config dist/db/config/config.js",
    "seed": "NODE_ENV=development sequelize-cli db:seed:all --seeders-path dist/db/seeders --config dist/db/config/config.js",
    "reset": "sequelize-cli db:migrate:undo:all --migrations-path dist/db/migrations --config dist/db/config/config.js && npm run migrate && npm run seed"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.812.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "dotenv": "^16.5.0",
    "next": "15.3.2",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "undici": "^7.9.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.15.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-restrict-imports": "^0.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
